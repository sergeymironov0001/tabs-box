{
  "manifest_version": 2,
  "name": "Tabs box",
  "description": "This extension allows the user to put tabs into tab boxes.",
  "version": "1.0",
  "icons": {
    "16": "icons/icon16.png",
    "24": "icons/icon24.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/tabs-box/boxes.js",
      "js/tabs-box/notifications.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon32.png",
    "default_title": "Tabs box",
    "default_popup": "html/popup.html"
  }
}
