{
  "manifest_version": 2,
  "name": "Tabs box",
  "description": "This extension allows the user to put tabs into tab boxes.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "popup.js"
    ]
  }
}
